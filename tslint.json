{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "quotemark": [
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "member-ordering": [
            false
        ],
        "no-console": [
            false
        ],
        "object-literal-sort-keys": false
    }
}