    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.15.3",
    "transit-immutable-js": "^0.7.0",
