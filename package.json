{
  "name": "aq-monitor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "better-npm-run build",
    "build:client": "webpack --config config/webpack/config.ts --env prod:client -p",
    "build:server": "webpack --config config/webpack/config.ts --env prod:server",
    "start": "node ./bin/prod",
    "prestart:dev": "npm run clean",
    "start:dev": "better-npm-run start:dev",
    "delay": "sleepms 10000",
    "start:dev:server-delayed": "npm-run-all delay start:dev:server",
    "start:dev:server": "nodemon --exec 'ts-node' bin/dev-server.ts -n",
    "start:webpack:server": "webpack --config config/webpack/config.ts --env dev:server --watch --hide-modules",
    "start:webpack:client": "ts-node bin/dev-client.ts -n"
  },
  "betterScripts": {
    "start:dev": {
      "command": "concurrently -c \"blue,green,magenta\" -n \"webpack:client,webpack:server,dev:server\" -p \"name\" \"npm run start:webpack:client\" \"npm run start:webpack:server\" \"npm run start:dev:server-delayed\"",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm-run-all build:client build:server",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
